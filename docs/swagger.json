    "definitions": {
        "surge.DeleteRuleDto": {
            "type": "object",
            "required": [
                "id"
            ],
            "properties": {
                "id": {
                    "type": "integer"
                }
            }
        },
        "surge.LatLonDto": {
            "type": "object",
            "required": [
                "lat",
                "lon"
            ],
            "properties": {
                "lat": {
                    "type": "number",
                    "example": 51.13199462890625
                },
                "lon": {
                    "type": "number",
                    "example": 35.73425097869431
                }
            }
        },
        "surge.RuleDto": {
            "type": "object",
            "required": [
                "coefficient",
                "threshold"
            ],
            "properties": {
                "coefficient": {
                    "type": "number",
                    "example": 1.12
                },
                "threshold": {
                    "type": "integer",
                    "example": 12
                }
            }
        },
        "user.UpdatePasswordDto": {
            "type": "object",
            "required": [
                "password"
            ],
            "properties": {
                "password": {
                    "description": "password must be longer than 4 chars",
                    "type": "string"
                }
            }
        },
        "user.UsersDto": {
            "type": "object",
            "required": [
                "password",
                "username"
            ],
            "properties": {
                "password": {
                    "description": "password must be longer than 4 chars",
                    "type": "string",
                    "example": "admin"
                },
                "username": {
                    "description": "username must be longer than 4 chars",
                    "type": "string",
                    "example": "admin"
                }
            }
        }
    },
    "securityDefinitions": {
        "Bearer": {
            "type": "apiKey",
            "name": "Authorization",
            "in": "header"
        }
    }
}